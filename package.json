{
  "name": "spdkey-tools",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start": "node bin/www",
    "dev": "./node_modules/.bin/nodemon bin/www",
    "format": "prettier --write \"src/**/*.js\" ",
    "prd": "pm2 start ./dist/index.js",
    "test": "jest --coverage",
    "lint": "eslint .",
    "build:single:file": "ncc build ./bin/www -m",
    "prod": "npm run build:single:file && npm run prd ",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^3.11.1",
    "ajv": "^8.11.0",
    "bluebird": "^3.7.2",
    "debug": "^4.1.1",
    "fs-extra": "^10.0.1",
    "ioredis": "^4.19.2",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "log4js": "^6.3.0",
    "shelljs": "^0.8.5",
    "uuid": "^8.3.1",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.32.0",
    "eslint-config-koa": "^2.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-standard": "^4.1.0",
    "husky": "^7.0.0",
    "jest": "^26.6.3",
    "nodemon": "^1.19.1",
    "prettier": "2.6.1",
    "pretty-quick": "^3.1.3",
    "prisma": "^3.11.1",
    "supertest": "^6.0.1"
  }
}
